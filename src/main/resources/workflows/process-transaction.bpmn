      <bpmn:incoming>Flow_1yl8gip</bpmn:incoming>
      <bpmn:outgoing>Flow_1fu6wkw</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_1b5eryb" name="NO" sourceRef="Gateway_02p1qx3" targetRef="Event_1bjx63p" />
